{
  "name": "@tscircuit/snippets-cli",
  "main": "dist/main.js",
  "type": "module",
  "version": "0.0.3",
  "bin": {
    "snippets": "./dist/main.js"
  },
  "scripts": {
    "start": "bun run dev",
    "dev": "bun run --hot ./cli/main.ts dev ./example-dir/snippet.tsx",
    "build": "tsup-node cli/main.ts --format esm --sourcemap inline",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tscircuit/core": "^0.0.247",
    "@types/bun": "latest",
    "@types/configstore": "^6.0.2",
    "@types/react": "^19.0.1",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@tscircuit/file-server": "^0.0.11",
    "@tscircuit/runframe": "^0.0.39",
    "chokidar": "^4.0.1",
    "commander": "^12.1.0",
    "configstore": "^7.0.0"
  }
}
